  },
  "keywords": [
    "env",
    "environment",
    "gist",
    "github"
  ],
  "author": "neurostunt@gmail.com",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/inquirer": "^8.2.6",
    "@types/node": "^20.11.5",
    "typescript": "^5.3.3"
  }
}
