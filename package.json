{
  "name": "gistenv",
  "version": "0.1.0",
  "description": "CLI tool to copy environment variables from GitHub Gists to local .env files",
  "main": "dist/index.js",
  "bin": {
    "gistenv": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "start": "node dist/cli.js",
